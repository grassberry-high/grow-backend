{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-len": [2,{"code":  240}],
      "indent": [
        "error",
        2,
        {"SwitchCase": 1}
      ],
      "no-invalid-this": "off"
    }
}
